/* pages/time/play.wxss */
.play{
    width: 4rem;
    height: 4rem;
    float: right;
    background: transparent url(data:image/jpeg;base64,) repeat; 
 background-size: 100%;
}
.stop{
   width: 4rem;
    height: 4rem;
    float: right;
    background: transparent url(data:image/jpeg;base64,) repeat;
  background-size: 110%;
}
.time {
    position: fixed;
    top: 2rem;
    left: 50%;
    font-size: 2rem;
    color: #fc2736;
    transform: translate(-50%, 0);
    z-index: 10;
}
.score {
    position: fixed;
    top: 5rem;
    left: 50%;
    font-size: 2rem;
    color: #fc2736;
    transform: translate(-50%, 0);
    z-index: 10;
}
.play-box {
    width: 100%;
    position: fixed;
    left: 5%;
    bottom: 0;
    background: transparent url(data:image/jpeg;base64,) repeat; 
    background-size: 100%; 
}
.block-line {
    width: 100%;
    overflow: hidden;        
}
.block {
    width: 5rem;
    height: 8rem;
    float: left;
    border: 1px solid #000000;
    box-sizing: border-box;
}
.black {
    background: #000000;
}
